{
  "name": "budgetport",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "initdb": "ts-node src/server/database/runQuery initdb",
    "viewdb": "ts-node src/server/database/runQuery viewdb",
    "dbcli": "docker exec -it budgetport-db psql -U postgres",
    "lint": "eslint . --ext .ts",
    "dev:client": "nodemon --watch src/client --ext ts --exec \"npm run restart:client\"",
    "dev:server": "nodemon --watch src/server --ext ts --exec \"npm run restart:server\"",
    "restart:client": "npm run build:client && npm run start:client",
    "restart:server": "npm run build:server && npm run start:server",
    "build:client": "rimraf ./build/client && tsc --project ./tsconfig.client.json",
    "build:server": "rimraf ./build/server && tsc --project ./tsconfig.server.json",
    "start:client": "node build/client/index.js",
    "start:server": "node build/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JobSavelsberg/Budgetport.git"
  },
  "author": "Job Savelsberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JobSavelsberg/Budgetport/issues"
  },
  "homepage": "https://github.com/JobSavelsberg/Budgetport#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "fs-extra": "^9.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}

{
  "name": "budgetport",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines" : { "node" : "10.x", "npm": "6.x" },
  "scripts": {
    "initdb": "ts-node src/server/database/runQuery initdb",
    "populatedb": "ts-node src/server/database/runQuery populatedb",
    "viewdb": "ts-node src/server/database/runQuery viewdb",
    "dbcli": "docker exec -it budgetport-db psql -U postgres",
    "backupdb": "docker exec -t budgetport-db pg_dumpall -c -U postgres > ./backup/dbdump.sql",
    "restoredb": "cat your_dump.sql | docker exec -i budgetport-db psql -U postgres",
    "lint": "eslint . --ext .ts",
    "dev:client": "nodemon --watch src/client --ext ts,vue,js --exec \"npm run rebuild:client\"",
    "dev:serverdb": "npm run start:db && npm run dev:server",
    "dev:server": "nodemon --watch src/server --ext ts --exec \"npm run restart:server\"",
    "restart:server": "npm run build:server && npm run start:server",
    "rebuild:client": "rimraf ./build/client && webpack",
    "build:client": "webpack",
    "build:server": "rimraf ./build/server && tsc --project ./tsconfig.server.json",
    "start:db": "docker start budgetport-db",
    "start:server": "node build/server/main.js",
    "build": "npm run build:server && webpack",
    "start": "node build/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JobSavelsberg/Budgetport.git"
  },
  "author": "Job Savelsberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JobSavelsberg/Budgetport/issues"
  },
  "homepage": "https://github.com/JobSavelsberg/Budgetport#readme",
  "dependencies": {
    "@types/vue-router": "^2.0.0",
    "axios": "^0.20.0",
    "big.js": "^5.2.2",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.21.1",
    "firebase-admin": "^9.2.0",
    "html-webpack-plugin": "^4.3.0",
    "material-design-icons-iconfont": "^5.0.1",
    "numeral": "^2.0.6",
    "pg": "^8.3.2",
    "serve-static": "^1.14.1",
    "sortablejs": "^1.12.0",
    "v-click-outside": "^3.0.1",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.5",
    "vue-nestable": "^2.6.0",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.4.4",
    "vuedraggable": "^2.24.1",
    "vuetify": "^2.3.9",
    "vuex": "^3.5.1"
